{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "zhenzh-adf"
		},
		"ADLSGen2LinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSGen2LinkedService'"
		},
		"CloudSalesLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudSalesLinkedService'"
		},
		"CloudSalesStreamingLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudSalesStreamingLinkedService'"
		},
		"CosmosDbLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbLinkedService'"
		},
		"FileServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer1'"
		},
		"ReportingDWLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ReportingDWLinkedService'"
		},
		"SqlServerLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerLinkedService'"
		},
		"MovieSinkDataset_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"MovieSinkDataset_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "raw"
		},
		"ADLSGen2LinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zhenzhmdwstorage.dfs.core.windows.net"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "openhackadmin"
		},
		"FourthCoffeeSelfHostedIR_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/e139b3f3-760f-4d78-8ea1-8c713669b878/resourcegroups/petsahack-rg/providers/Microsoft.DataFactory/factories/petsahavtory/integrationruntimes/integrationRuntime1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSGen2LinkedService_accountKey": {
						"value": "[parameters('ADLSGen2LinkedService_accountKey')]"
					},
					"CloudSalesLinkedService_connectionString": {
						"value": "[parameters('CloudSalesLinkedService_connectionString')]"
					},
					"CloudSalesStreamingLinkedService_connectionString": {
						"value": "[parameters('CloudSalesStreamingLinkedService_connectionString')]"
					},
					"CosmosDbLinkedService_connectionString": {
						"value": "[parameters('CosmosDbLinkedService_connectionString')]"
					},
					"ReportingDWLinkedService_connectionString": {
						"value": "[parameters('ReportingDWLinkedService_connectionString')]"
					},
					"FileServer1_password": {
						"value": "[parameters('FileServer1_password')]"
					},
					"SqlServerLinkedService_connectionString": {
						"value": "[parameters('SqlServerLinkedService_connectionString')]"
					},
					"ADLSGen2LinkedService_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen2LinkedService_properties_typeProperties_url')]"
					},
					"FourthCoffeeSelfHostedIR_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('FourthCoffeeSelfHostedIR_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"MovieSinkDataset_properties_typeProperties_fileName": {
						"value": "[parameters('MovieSinkDataset_properties_typeProperties_fileName')]"
					},
					"MovieSinkDataset_properties_typeProperties_folderPath": {
						"value": "[parameters('MovieSinkDataset_properties_typeProperties_folderPath')]"
					},
					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}